{
  "openapi": "3.1.0",
  "info": {
    "title": "IntelliOptics API",
    "version": "0.1.0"
  },
  "servers": [
    {
      "url": "http://localhost:8000"
    }
  ],
  "paths": {
    "/healthz": {
      "get": {
        "summary": "Health check",
        "operationId": "healthz_healthz_get",
        "responses": {
          "200": {
            "description": "Service is healthy",
            "content": {
              "application/json": {
                "schema": {
                  "oneOf": [
                    {
                      "type": "object",
                      "additionalProperties": true
                    },
                    {
                      "type": "string"
                    }
                  ]
                }
              }
            }
          },
          "default": {
            "description": "Unexpected error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/v1/users/me": {
      "get": {
        "summary": "Who am I",
        "operationId": "whoami_v1_users_me_get",
        "responses": {
          "200": {
            "description": "Authenticated user info",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/UserIdentity"
                }
              }
            }
          },
          "default": {
            "description": "Unexpected error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/v1/detectors": {
      "get": {
        "summary": "List detectors",
        "operationId": "list_detectors_v1_detectors_get",
        "responses": {
          "200": {
            "description": "Detector list",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/DetectorList"
                }
              }
            }
          },
          "default": {
            "description": "Unexpected error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      },
      "post": {
        "summary": "Create detector",
        "operationId": "create_detector_v1_detectors_post",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/DetectorCreate"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Created detector",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/DetectorOut"
                }
              }
            }
          },
          "default": {
            "description": "Unexpected error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/v1/detectors/{detector_id}": {
      "get": {
        "summary": "Get detector",
        "operationId": "get_detector_v1_detectors_detector_id_get",
        "parameters": [
          {
            "name": "detector_id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Detector details",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/DetectorOut"
                }
              }
            }
          },
          "default": {
            "description": "Unexpected error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/v1/image-queries": {
      "post": {
        "summary": "Submit image query (multipart)",
        "operationId": "image_query_v1_image_queries_post",
        "requestBody": {
          "required": true,
          "content": {
            "multipart/form-data": {
              "schema": {
                "$ref": "#/components/schemas/ImageQueryMultipart"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Submitted image query",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/AnswerOut"
                }
              }
            }
          },
          "default": {
            "description": "Unexpected error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/v1/image-queries/json": {
      "post": {
        "summary": "Submit image query (JSON)",
        "operationId": "image_query_json_v1_image_queries_json_post",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ImageQueryJson"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Submitted image query",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/AnswerOut"
                }
              }
            }
          },
          "default": {
            "description": "Unexpected error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/v1/image-queries/{iq_id}": {
      "get": {
        "summary": "Get image query",
        "operationId": "get_image_query_v1_image_queries_iq_id_get",
        "parameters": [
          {
            "name": "iq_id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Image query details",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/AnswerOut"
                }
              }
            }
          },
          "default": {
            "description": "Unexpected error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/v1/feedback": {
      "post": {
        "summary": "Submit feedback",
        "operationId": "feedback_v1_feedback_post",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/FeedbackIn"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Acknowledged",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "additionalProperties": true
                }
              }
            }
          },
          "default": {
            "description": "Unexpected error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/v1/labels": {
      "post": {
        "summary": "Add label",
        "operationId": "add_label_v1_labels_post",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/LabelCreate"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Label created",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/LabelRecord"
                }
              }
            }
          },
          "default": {
            "description": "Unexpected error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "DetectorCreate": {
        "type": "object",
        "required": [
          "name"
        ],
        "properties": {
          "name": {
            "type": "string"
          },
          "labels": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "default": []
          }
        }
      },
      "DetectorOut": {
        "allOf": [
          {
            "$ref": "#/components/schemas/DetectorCreate"
          },
          {
            "type": "object",
            "required": [
              "id"
            ],
            "properties": {
              "id": {
                "type": "string"
              }
            }
          }
        ]
      },
      "DetectorList": {
        "type": "object",
        "required": [
          "items"
        ],
        "properties": {
          "items": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/DetectorOut"
            }
          }
        }
      },
      "ImageQueryMultipart": {
        "type": "object",
        "required": [
          "detector_id"
        ],
        "properties": {
          "detector_id": {
            "type": "string"
          },
          "wait": {
            "type": "number",
            "nullable": true
          },
          "confidence_threshold": {
            "type": "number",
            "nullable": true
          },
          "human_review": {
            "type": "string",
            "nullable": true
          },
          "metadata": {
            "type": "string",
            "nullable": true
          },
          "inspection_id": {
            "type": "string",
            "nullable": true
          },
          "image": {
            "type": "string",
            "format": "binary",
            "nullable": true
          }
        }
      },
      "ImageQueryJson": {
        "type": "object",
        "required": [
          "detector_id"
        ],
        "properties": {
          "detector_id": {
            "type": "string"
          },
          "image": {
            "type": "string",
            "nullable": true
          },
          "wait": {
            "type": "number",
            "nullable": true
          },
          "confidence_threshold": {
            "type": "number",
            "nullable": true
          },
          "human_review": {
            "type": "string",
            "nullable": true
          },
          "metadata": {
            "type": "object",
            "nullable": true
          },
          "inspection_id": {
            "type": "string",
            "nullable": true
          }
        }
      },
      "AnswerOut": {
        "type": "object",
        "required": [
          "image_query_id",
          "answer",
          "confidence"
        ],
        "properties": {
          "image_query_id": {
            "type": "string"
          },
          "answer": {
            "type": "string"
          },
          "confidence": {
            "type": "number"
          },
          "latency_ms": {
            "type": "integer",
            "nullable": true
          },
          "model_version": {
            "type": "string",
            "nullable": true
          }
        }
      },
      "FeedbackIn": {
        "type": "object",
        "required": [
          "iq_id",
          "correct_label"
        ],
        "properties": {
          "iq_id": {
            "type": "string"
          },
          "correct_label": {
            "type": "string"
          },
          "bboxes": {
            "type": "array",
            "items": {
              "type": "object",
              "additionalProperties": true
            },
            "nullable": true
          },
          "notes": {
            "type": "string",
            "nullable": true
          }
        }
      },
      "LabelCreate": {
        "type": "object",
        "required": [
          "image_query_id",
          "label"
        ],
        "properties": {
          "image_query_id": {
            "type": "string"
          },
          "label": {
            "type": "string"
          },
          "confidence": {
            "type": "number",
            "nullable": true
          },
          "detector_id": {
            "type": "string",
            "nullable": true
          },
          "user_id": {
            "type": "string",
            "nullable": true
          },
          "metadata": {
            "type": "object",
            "additionalProperties": true,
            "nullable": true
          }
        }
      },
      "LabelRecord": {
        "allOf": [
          {
            "$ref": "#/components/schemas/LabelCreate"
          },
          {
            "type": "object",
            "properties": {
              "id": {
                "type": "string",
                "nullable": true
              },
              "created_at": {
                "type": "string",
                "format": "date-time",
                "nullable": true
              }
            }
          }
        ]
      },
      "UserIdentity": {
        "type": "object",
        "required": [
          "id"
        ],
        "properties": {
          "id": {
            "type": "string"
          },
          "email": {
            "type": "string",
            "nullable": true
          }
        }
      },
      "ValidationError": {
        "type": "object",
        "required": [
          "loc",
          "msg",
          "type"
        ],
        "properties": {
          "loc": {
            "type": "array",
            "items": {
              "oneOf": [
                {
                  "type": "string"
                },
                {
                  "type": "integer"
                }
              ]
            }
          },
          "msg": {
            "type": "string"
          },
          "type": {
            "type": "string"
          }
        }
      },
      "HTTPValidationError": {
        "type": "object",
        "properties": {
          "detail": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            }
          }
        }
      }
    }
  }
}
